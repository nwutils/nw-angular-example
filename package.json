{
  "name": "nw-angular-example",
  "version": "18.1.4",
  "main": "http://localhost:4200",
  "node-remote": "http://localhost:4200",
  "window": {
    "width": 960,
    "height": 600,
    "min_width": 700,
    "min_height": 500,
    "icon": "assets/icon.png"
  },
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "test": "ng test --watch=false",
    "start": "concurrently -k npm:dev:ng npm:dev:nw",
    "postinstall": "base-volta-off-of-nwjs",
    "dev:ng": "ng serve",
    "dev:nw": "wait-on -c waitOn.config.json http-get://localhost:4200 && nw .",
    "bld": "npm run bld:ng && npm run bld:nw",
    "bld:ng": "ng build --output-path ./out/ng",
    "bld:nw": "nwbuild --cacheDir=./node_modules/nw --outDir=./out/nw --managedManifest=./package.nw.json ./package.json ./out/ng/*"
  },
  "private": true,
  "volta": {
    "npm": "10.8.2",
    "node": "22.2.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~20.1.6",
    "@angular-eslint/builder": "~20.1.1",
    "@angular-eslint/eslint-plugin": "~20.1.1",
    "@angular-eslint/eslint-plugin-template": "~20.1.1",
    "@angular-eslint/schematics": "~20.1.1",
    "@angular-eslint/template-parser": "~20.1.1",
    "@angular/animations": "~20.1.7",
    "@angular/cli": "~20.1.6",
    "@angular/common": "~20.1.7",
    "@angular/compiler": "~20.1.7",
    "@angular/compiler-cli": "~20.1.7",
    "@angular/core": "~20.1.7",
    "@angular/forms": "~20.1.7",
    "@angular/language-service": "~20.1.7",
    "@angular/platform-browser": "~20.1.7",
    "@angular/platform-browser-dynamic": "~20.1.7",
    "@angular/router": "~20.1.7",
    "@types/chrome": "^0.1.4",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^24.3.0",
    "@types/nw.js": "^0.92.0",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "@typescript-eslint/types": "^8.0.1",
    "@typescript-eslint/utils": "^8.0.1",
    "base-volta-off-of-nwjs": "^1.0.3",
    "concurrently": "^9.0.1",
    "eslint": "^9.9.1",
    "eslint-plugin-jsdoc": "^54.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "nw": "^0.102.1",
    "nw-builder": "^4.8.1",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "typescript": "~5.5.4",
    "wait-on": "^8.0.0",
    "zone.js": "~0.15.0"
  },
  "chromium-args": "--enable-spell-checking"
}
