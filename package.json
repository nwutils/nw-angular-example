{
  "name": "nw-angular-example",
  "version": "17.3.0",
  "main": "http://localhost:4200",
  "node-remote": "http://localhost:4200",
  "window": {
    "width": 960,
    "height": 600,
    "min_width": 700,
    "min_height": 500,
    "icon": "assets/icon.png"
  },
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "test": "ng test --watch=false",
    "start": "concurrently -k npm:dev:ng npm:dev:nw",
    "postinstall": "base-volta-off-of-nwjs",
    "dev:ng": "ng serve",
    "dev:nw": "wait-on -c waitOn.config.json http-get://localhost:4200 && nw .",
    "bld": "npm run bld:ng && npm run bld:nw",
    "bld:ng": "ng build --output-path ./out/ng",
    "bld:nw": "nwbuild --cacheDir=./node_modules/nw --outDir=./out/nw --managedManifest=./package.nw.json ./package.json ./out/ng/*"
  },
  "private": true,
  "volta": {
    "node": "22.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.4",
    "@angular-eslint/builder": "^18.0.1",
    "@angular-eslint/eslint-plugin": "^18.0.1",
    "@angular-eslint/eslint-plugin-template": "^18.0.1",
    "@angular-eslint/schematics": "^18.0.1",
    "@angular-eslint/template-parser": "^18.0.1",
    "@angular/animations": "^18.0.3",
    "@angular/cli": "^18.0.4",
    "@angular/common": "^18.0.3",
    "@angular/compiler": "^18.0.3",
    "@angular/compiler-cli": "^18.0.3",
    "@angular/core": "^18.0.3",
    "@angular/forms": "^18.0.3",
    "@angular/language-service": "^18.0.3",
    "@angular/platform-browser": "^18.0.3",
    "@angular/platform-browser-dynamic": "^18.0.3",
    "@angular/router": "^18.0.3",
    "@types/chrome": "^0.0.268",
    "@types/jasmine": "^5.1.4",
    "@types/node": "^20.12.11",
    "@types/nw.js": "^0.13.21",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@typescript-eslint/utils": "^7.11.0",
    "base-volta-off-of-nwjs": "^1.0.3",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jasmine-core": "^5.1.2",
    "karma": "^6.4.3",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "nw": "0.88.0-3",
    "nw-builder": "^4.7.4",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "typescript": "~5.4.3",
    "wait-on": "^7.2.0",
    "zone.js": "~0.14.5"
  },
  "chromium-args": "--enable-spell-checking"
}
